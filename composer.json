{
    "name": "parhamafkar/laravel-mattermost-logger",
    "description": "Laravel 11 package to log all system logs to Mattermost",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Parham Afkar",
            "email": "ParhamAfkar@Riseup.Net"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0",
        "monolog/monolog": "^3.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "ParhamAfkar\\MattermostLogger\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ParhamAfkar\\MattermostLogger\\MattermostServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
